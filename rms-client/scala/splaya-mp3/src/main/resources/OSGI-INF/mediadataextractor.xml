<?xml version="1.0" encoding="UTF-8"?>
<!--
  Declarative Services declaration for the ID3MediaDataExtractor component.
  This component provides support for extracting meta data from MP3 audio
  files by parsing their ID3 tags.
-->
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0"
  name="MP3MediaDataExtractor">
  <scr:implementation class="de.oliver_heger.splaya.mp3.MP3MediaDataExtractor" />

  <scr:service>
    <provide interface="de.oliver_heger.splaya.MediaDataExtractor"/>
  </scr:service>
</scr:component>
